{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist-8",
    "rootDir": "./src-8",
    "module": "ES2020",
    "target": "ES2020",
    "moduleResolution": "Bundler",
    "noEmitOnError": true,
    "allowImportingTsExtensions": false
  },
  "include": ["src-8/**/*.ts"]
}
